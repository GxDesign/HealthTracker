                                        "widget-icon widget-icon-top bg-default-light to-center">
                                        <div id="calories-mini-chart"></div>
                                        </div><!-- Accordion In Panel -->
                                        <div class="panel to-center">
                                            <div class="panel-heading">
                                                <div class="panel-title dark">
                                                    Total
                                                    <strong>Calories</strong>
                                                    for the past five days
                                                </div>
                                            </div>
                                            <div class="panel-body panel-full">
                                                <!-- start of calories form -->
                                                <%= form_for(:calories, html: {id: "calories_form"}) do |f| %>
                                                <div aria-multiselectable=
                                                "false" class=
                                                "accordion panel-group no-mb"
                                                id="accordion-1" role=
                                                "tablist">
                                                    
                                                    <div class=
                                                    "panel panel-inverse">
                                                        <div class=
                                                        "panel-heading" id=
                                                        "heading-1" role="tab">
                                                            <h5 class=
                                                            "panel-title">
                                                            <a aria-controls=
                                                            "collapse-1"
                                                            aria-expanded=
                                                            "false" class=
                                                            "accordion-toggler"
                                                            data-parent=
                                                            "#accordion-1"
                                                            data-toggle=
                                                            "collapse" href=
                                                            "#collapse-1" role=
                                                            "button">
                                                            USDA Search</a></h5>
                                                        </div>
                                                        <div aria-labelledby=
                                                        "heading-1" class=
                                                        "panel-collapse collapse"
                                                        id="collapse-1" role=
                                                        "tabpanel">
                                                            <div class=
                                                            "panel-body dark total_calories">
                                                            <div id="total_calories">
                                                                <strong> Total Calories: </strong> 
                                                                <p>0</p>
                                                            </div>
                                                                <%= label_tag :groups, "Narrow Food Scope"  %></br>
                                                                <%= select_tag(:group, options_for_select(FoodGroup.all.collect {|group| [group.name, group.id]}), {include_blank: true, style: "display:  block; width: 100%;"} ) %></br>
                                                                <%= label_tag :foods, "Add Foods"  %></br>
                                                                <%= text_area_tag :foods %> </br>

                                                                <%= button_tag(type: 'button', id: 'calculate_food', class: 'btn-primary') do %>
                                                                    <%= content_tag(:strong, 'Add to Total') %>
                                                                <% end %> </br><br>
                                                                <div class="actions">
                                                                  <%= f.submit class: 'btn-primary' %>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                
                                                <div aria-multiselectable=
                                                "false" class=
                                                "accordion panel-group no-mb"
                                                id="accordion-1" role=
                                                "tablist">
                                                    
                                                    <div class=
                                                    "panel panel-inverse">
                                                        <div class=
                                                        "panel-heading" id=
                                                        "heading-2" role="tab">
                                                            <h5 class=
                                                            "panel-title">
                                                            <a aria-controls=
                                                            "collapse-2"
                                                            aria-expanded=
                                                            "false" class=
                                                            "accordion-toggler"
                                                            data-parent=
                                                            "#accordion-1"
                                                            data-toggle=
                                                            "collapse" href=
                                                            "#collapse-2" role=
                                                            "button">
                                                            Costum Calories</a></h5>
                                                        </div>
                                                        <div aria-labelledby=
                                                        "heading-2" class=
                                                        "panel-collapse collapse"
                                                        id="collapse-2" role=
                                                        "tabpanel">
                                                            <div class=
                                                            "panel-body dark total_calories">
                                                            <strong>Total
                                                            Calories:</strong>
                                                                <p>
                                                                0</p><%= text_field_tag :costum_calories %><br>

                                                                <br>
                                                                <%= form_for(:user_stat, html: {id: "calories_form"}) do |f| %>
                                                                    <%= button_tag(type: 'button', id: 'calculate_custom', class: 'btn-primary') do %>
                                                                    <%= content_tag(:strong, 'Add to Total') %>
                                                                    <%= f.hidden_field :calories, value: 0 %>
                                                                    <%end %></br>
                                                                    <br>
                                                                    <div class="actions">
                                                                         <%= f.submit class: 'btn-primary' %>
                                                                    </div>
                                                                <% end %>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <% end %>
